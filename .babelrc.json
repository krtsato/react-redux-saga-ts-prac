{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": [">5% in JP", "not ie < 11", "not op_mini all"]
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "babel-plugin-import",
      {
        "libraryName": "@material-ui/core",
        "libraryDirectory": "esm",
        "camel2DashComponentName": false
      },
      "core"
    ]
  ]
}
